<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HomyjobBundle\Entity\Hashtag" table="hashtag">
    <id name="hashtagId" type="integer" column="hashtag_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hashtagValue" type="string" column="hashtag_value" length="30" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">valeur du hashtag</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
